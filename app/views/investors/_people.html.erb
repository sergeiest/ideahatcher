<% div_name='investor' + people.firstname + people.lastname %>

<%= link_to :controller => 'users', :id => people.id do %>
  <div class="person-info">

     <% if  people.avatar? %>
        <img src='<%= people.avatar.url(:square) %>' >
     <% else %>
        <img src='/images/profile/Nophoto.png'>
     <% end %>

     <h5><%= people.firstname %>  <%= people.lastname %></h5>

   <div class="clear-both"></div>

  </div>
<% end %>
